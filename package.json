    "font-awesome": "^4.7.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "qs": "^6.7.0",
    "rc-select": "^9.1.0",
    "react": "^16.8.6",
    "react-css-loaders": "^0.0.5",
    "react-dom": "^16.8.6",
    "react-facebook-login": "^4.1.1",
    "react-pagination-library": "^0.1.14",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.8",
    "react-select": "^2.4.2",
    "react-stripe-elements": "^2.0.3",
